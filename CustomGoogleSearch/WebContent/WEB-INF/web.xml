  </welcome-file-list>
  <servlet>
    <servlet-name>a</servlet-name>
    <servlet-class>MySearcher</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>a</servlet-name>
    <url-pattern>/MySearcher</url-pattern>
  </servlet-mapping>
</web-app>